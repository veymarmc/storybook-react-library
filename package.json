{
  "name": "@vmc-react-js/storybook-react-library",
  "description": "Ready to use useful react components.",
  "version": "0.1.2",
  "private": false,
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/veymarmc/storybook-react-library.git"
  },
  "keywords": [
    "react",
    "components",
    "storybook",
    "webpack"
  ],
  "scripts": {
    "start": "react-scripts start",
    "build:react": "react-scripts build",
    "build": "rm -rf dist && webpack --config ./webpack.config.lib.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "peerDependencies": {
    "react": "^18.1.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.18.0",
    "@babel/preset-react": "^7.17.12",
    "@storybook/addon-actions": "^6.5.3",
    "@storybook/addon-essentials": "^6.5.3",
    "@storybook/addon-interactions": "^6.5.3",
    "@storybook/addon-links": "^6.5.3",
    "@storybook/builder-webpack5": "^6.5.3",
    "@storybook/manager-webpack5": "^6.5.3",
    "@storybook/node-logger": "^6.5.3",
    "@storybook/preset-create-react-app": "^4.1.0",
    "@storybook/react": "^6.5.3",
    "@storybook/testing-library": "^0.0.11",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@testing-library/user-event": "^13.5.0",
    "babel-plugin-named-exports-order": "^0.0.2",
    "css-loader": "^6.7.1",
    "mini-css-extract-plugin": "^2.6.0",
    "prop-types": "^15.8.1",
    "react-dom": "^18.1.0",
    "react-scripts": "5.0.1",
    "sass": "^1.52.1",
    "sass-loader": "^13.0.0",
    "web-vitals": "^2.1.4",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2"
  }
}
